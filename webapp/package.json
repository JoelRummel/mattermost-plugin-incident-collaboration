{
  "private": true,
  "scripts": {
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=production --watch",
    "debug": "webpack --mode=development",
    "debug:watch": "webpack --mode=development --watch",
    "lint": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js --ext .jsx --ext tsx --ext ts . --quiet --cache",
    "fix": "eslint --ignore-pattern node_modules --ignore-pattern dist --ext .js --ext .jsx --ext tsx --ext ts . --quiet --fix --cache",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:watch": "jest --watch",
    "test-ci": "jest --forceExit --detectOpenHandles --maxWorkers=2",
    "check-types": "tsc"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@emotion/core": "^10.0.28",
    "@testing-library/jest-dom": "5.11.9",
    "@types/enzyme": "3.10.8",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@types/react": "16.9.49",
    "@types/react-beautiful-dnd": "13.0.0",
    "@types/react-bootstrap": "0.32.22",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router-dom": "5.1.5",
    "@types/react-select": "3.0.19",
    "@types/react-test-renderer": "^16.9.3",
    "@types/react-transition-group": "4.4.0",
    "@types/shallow-equals": "1.0.0",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-typescript-to-proptypes": "1.4.2",
    "classnames": "^2.2.6",
    "compass-mixins": "^0.12.10",
    "css-loader": "5.0.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.19.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "file-loader": "6.2.0",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.0",
    "jest-junit": "12.0.0",
    "node-sass": "5.0.0",
    "react-beautiful-dnd": "13.0.0",
    "react-bootstrap": "github:mattermost/react-bootstrap#c6957962364e0818a51bbfd13e89919903b422d6",
    "react-test-renderer": "^17.0.1",
    "sass-loader": "11.0.1",
    "style-loader": "2.0.0",
    "webpack": "5.20.2",
    "webpack-cli": "4.5.0"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.28",
    "@types/chart.js": "2.9.21",
    "@types/classnames": "^2.2.10",
    "@types/debounce": "^1.2.0",
    "@types/qs": "6.9.3",
    "@types/react-custom-scrollbars": "^4.0.6",
    "@types/react-redux": "7.1.9",
    "@types/react-select": "3.0.13",
    "chart.js": "2.9.4",
    "core-js": "3.8.3",
    "debounce": "^1.2.0",
    "mattermost-webapp": "github:mattermost/mattermost-webapp#f3cc29e4d60dd24e4c31d777f7f9d2288d1d34ad",
    "moment": "2.26.0",
    "qs": "6.9.4",
    "react": "16.13.1",
    "react-bootstrap": "^1.0.1",
    "react-chartjs-2": "2.11.1",
    "react-redux": "7.2.1",
    "react-select": "3.1.1",
    "redux": "4.0.5",
    "reselect": "4.0.0",
    "styled-components": "5.1.1",
    "typescript": "4.1.3"
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/non_npm_dependencies/"
    ],
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleNameMapper": {
      "^mattermost-redux/test/(.*)$": "<rootDir>/node_modules/mattermost-webapp/packages/mattermost-redux/test/$1",
      "^mattermost-redux/(.*)$": "<rootDir>/node_modules/mattermost-webapp/packages/mattermost-redux/src/$1",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy",
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "^.*i18n.*\\.(json)$": "<rootDir>/tests/i18n_mock.json",
      "^bundle-loader\\?lazy\\!(.*)$": "$1"
    },
    "moduleDirectories": [
      "",
      "node_modules",
      "non_npm_dependencies"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-router|mattermost-webapp)"
    ],
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "testURL": "http://localhost:8065"
  },
  "jest-junit": {
    "output": "build/test-results.xml"
  }
}
